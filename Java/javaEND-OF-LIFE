import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner scan = new Scanner(System.in); 
    int i=1;
    String s;
    while (scan.hasNextLine()){
        s=scan.nextLine();
        System.out.print((i++)+" "+s+"\n");

    }
       
    }
}
